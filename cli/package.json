{
  "name": "foran",
  "version": "0.1.0",
  "description": "A CLI tool for code forensics analysis",
  "main": "dist/index.js",
  "bin": {
    "foran": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "start": "node dist/index.js"
  },
  "keywords": ["cli", "code-forensics", "analysis", "git"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.10.0",
    "commander": "^11.1.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.8.0",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
